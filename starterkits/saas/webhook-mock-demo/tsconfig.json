{
  "compilerOptions": {
    "target": "es2022",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["../../../webhook-mock-demo/*"]
    }
  },
  "include": ["../../../webhook-mock-demo/next-env.d.ts", "../../../webhook-mock-demo/**/*.ts", "../../../webhook-mock-demo/**/*.tsx", "../../../webhook-mock-demo/.next/types/**/*.ts"],
  "exclude": ["../../../webhook-mock-demo/node_modules"]
}
